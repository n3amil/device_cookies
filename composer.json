{
  "name": "neamil/device_cookies",
  "description": "a base for a php implementation of device cookies as described in OWASP https://www.owasp.org/index.php/Slow_Down_Online_Guessing_Attacks_with_Device_Cookies",
  "require": {
    "php": "^7.2"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "^8.4",
    "ext-xdebug": "*",
    "phpstan/phpstan": "^0.12.26",
    "vimeo/psalm": "^3.11",
    "friendsofphp/php-cs-fixer": "v2.16.3"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Johannes Seipelt",
      "email": "johannes.seipelt@3m5.de"
    }
  ],
  "autoload": {
    "psr-4": {
      "Neamil\\DeviceCookies\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Neamil\\DeviceCookies\\Tests\\": "tests"
    }
  }
}
